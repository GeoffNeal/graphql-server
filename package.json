{
  "name": "graphql-server",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "scripts": {
    "compile": "tsc && webpack",
    "start:server": "node ./dist/server/index.js",
    "start:client": "webpack-dev-server ./dist/client/index.js",
    "start": "yarn compile && concurrently \"yarn start:server\" \"yarn start:client\"",
    "create-db": "psql -U postgres < ./src/db/schema.sql",
    "seed": "yarn compile && yarn create-db && node ./dist/db/seed.js --product ../../data/product.csv --customer ../../data/customer.csv"
  },
  "dependencies": {
    "@apollo/client": "^3.8.2",
    "@apollo/server": "^4.9.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.0",
    "http-server": "^14.1.1",
    "minimist": "^1.2.8",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@types/pg-pool": "^2.0.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "html-webpack-plugin": "^5.5.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
