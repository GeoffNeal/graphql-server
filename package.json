{
  "name": "graphql-server",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "scripts": {
    "build:server": "webpack --config webpack.config.server.js",
    "build:client": "webpack --config webpack.config.client.js",
    "build": "yarn build:server && yarn build:client",
    "start:server": "node ./dist/server/server.bundle.js",
    "start:client": "node ./dist/server/webserver.bundle.js",
    "start": "yarn build && concurrently \"yarn start:server\" \"yarn start:client\"",
    "create-db": "psql -U postgres < ./server/src/db/schema.sql",
    "seed": "yarn build:server && yarn create-db && node ./dist/server/seed.bundle.js",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.2",
    "@apollo/server": "^4.9.3",
    "classnames": "^2.3.2",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.4.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "pg": "^8.11.3",
    "pg-native": "^3.0.1",
    "pg-pool": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-modules-commonjs": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@jest/globals": "^29.6.4",
    "@types/jest": "^29.5.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@types/pg-pool": "^2.0.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.4",
    "mini-css-extract-plugin": "^2.7.6",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
